{
  "compilerOptions": {
    "target": "ES2022",
    "module": "commonjs",
    "types": ["node"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "rootDir": "./",
    "baseUrl": "./"
  },
  "include": ["src/**/*", "index.ts", "prisma/**/*.ts"],
  "exclude": ["node_modules", "**/*.test.ts"]
}
